{
  "name": "rho-cc-uri",
  "description": "Check URIs with rho-contracts",
  "version": "2.0.0",
  "license": "MPL-2.0",
  "main": "uri.js",
  "scripts": {
    "jscs": "jscs --config node_modules/bodylabs-javascript-style/jscs/jscsrc-shared *.js",
    "eslint": "eslint --config node_modules/bodylabs-javascript-style/eslint/eslintrc-shared.yml '*.js'",
    "lint": "npm run jscs && npm run eslint",
    "unittest": "mocha '*.spec.js'",
    "test": "npm run unittest && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bodylabs/rho-cc-uri.git"
  },
  "dependencies": {
    "underscore": "^1.8.3",
    "valid-url": "^1.0.9"
  },
  "peerDependencies": {
    "rho-contracts-fork": "*"
  },
  "devDependencies": {
    "bodylabs-javascript-style": "^4.0.0",
    "eslint": "~2.4.0",
    "jscs": "^2.8.0",
    "mocha": "~2.0.1",
    "rho-contracts-fork": "^1.3.0",
    "should": "~13.2.3"
  }
}
